{
  "RealtimeMap": {
    "SharedSubscriptionGroupName": null
  },
  "ProtoActor": {
    "DeveloperLogging": false
  },

  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override" : {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message} ({SourceContext}){NewLine}{Exception}"
        }
      }
    ]
  }

}
